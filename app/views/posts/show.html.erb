 <div id ="post_content">
 <h1 class="title">
 <%= @post.title%>
 </h1>
 <p calss="date">
 Submitted <%= time_ago_in_words(@post.created_at)%> ago | 
  <%= link_to 'Edit', edit_post_path(@post)%> | 
 <%= button_to 'Destroy', @post, method: :delete, :onclick => "return confirm('Are you sure you want to delete this post?')" %>
 </p>

 <p class ="body">
 <%=@post.body%>
 </p> 
 </div>